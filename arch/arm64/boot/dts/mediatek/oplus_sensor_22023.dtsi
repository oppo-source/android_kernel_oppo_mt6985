&odm {
	sensor_fb: sensor_fb {
		compatible = "oplus,sensor-feedback";
	};
	ssc_interactive: ssc_interactive {
		report_brightness = <1>;
		support_pwm_turbo = <1>;
		support_bri_to_scp = <1>;
	};
	oplus_sensor:oplus_sensor {
		compatible = "oplus,sensor-devinfo";
		als-row-coe = <1000>;
		/* sensor start */
		/*
		enum {
			LIS3DH,
			LSM6DS3,
			BMI160,
			LIS2HH12,
			LSM6DSM,
			BMI2XY,
			LSM6DSO,
			ICM4X6XX,
			ICM4263X,
			ICM4N607,
			BMI22X,
			SC7A20,
			MC34X9,
		};
		*/
		gsensor_1: gsensor_1 {
			status = "okay";
			sensor_name = <9>;//ICM4N607
			device_name = "icm4n607";
			i2c_num = <3>;
			i2c_addr = <0x68>;
			direction = <0>;
			eint_num = <16>;
			gyro_cali_version= <2>; //should be always v2 for lsm6dso
			gyro_cali_range = <350>;
			acc_cali_range= <300 300 300>;
			max_gyro_bias = <70>;
		};
		sub_gsensor_1: sub_gsensor_1 {
			status = "okay";
			sensor_name = <8>;//ICM4263X
			device_name = "icm4263x";
			i2c_num = <3>;
			i2c_addr = <0x69>;
			direction = <3>;
			eint_num = <20>;
			gyro_cali_version= <2>; //should be always v2 for lsm6dso
			gyro_cali_range = <350>;
			acc_cali_range= <300 300 300>;
			ois_pull_down = <0x00>;
		};
		sub_gsensor_2: sub_gsensor_2 {
			status = "okay";
			sensor_name = <5>;//BMI2XY
			device_name = "BMI2XY";
			i2c_num = <3>;
			i2c_addr = <0x69>;
			direction = <2>;
			eint_num = <20>;
			gyro_cali_version= <2>; //should be always v2 for lsm6dso
			gyro_cali_range = <350>;
			acc_cali_range= <300 300 300>;
		};
		/*
		enum {
			AKM09911,
			MMC3530,
			MMC5603,
			MXG4300,
			AKM09918,
		};
		*/
		msensor_1: msensor_1 {
			status = "okay";
			sensor_name = <4>;//AKM09918
			i2c_num = <6>;
			i2c_addr = <0x0C>;
			direction = <2>;
			eint_num = <0>;
			support_fold_state = <1>;
			soft-mag-parameter = <14506 0 100 0 740 0 671 1 10957 0 420 0 4974 0 2320 1 10273 0
						15520 0 668 1 700 0 2130 1 13083 0 611 1 4700 0 744 0 9672 0>;
		};
		/*
		enum {
			TMD2725,
			APSD9922,
			STK3335,
			STK3331,
			STK2232,
			TCS3701,
			STK32600,
			STK33502,
			MN78911,
			TCS3720,
		};
		*/
		proximity_1: proximity_1 {
			status = "okay";
			sensor_name = <9>;//TCS3720
			i2c_num = <5>;
			i2c_addr = <0x39>;
			direction = <0>;
			eint_num = <19>;
			low_step = <300>;
			high_step = <400>;
			low_limit = <400>;
			high_limit = <600>;
			dirty_low_step = <180>;
			dirty_high_step = <280>;
			ps_dirty_limit = <1800>;
			ps_ir_limit = <500>;
			ps_adjust_min = <0>;
			ps_adjust_max = <800>;
			sampling_count = <5>;
			step_max = <400>;
			step_min = <100>;
			step_div = <2000>;
			anti_shake_delta = <120>;
			dynamic_cali_max = <1500>;
			raw2offset_radio = <1000>;
			offset_max = <15000>;
			offset_range_min = <0>;
			offset_range_max = <65534>;
			force_cali_limit = <0>;
			cali_jitter_limit = <40>;
			cal_offset_margin = <3000>;
			ps_check_num = <3>;
			ps_mid_filter = <0>;
			sensor-reg = <0x82 0x43 0xD9 0xB9>;
		};
		rear_prox_1: rear_prox_1 {
			status = "okay";
			sensor_name = <7>; //STK33502
			i2c_num = <0>;
			i2c_addr = <0x46>;
			direction = <0>;
			eint_num = <12>;
			low_step = <200>;
			high_step = <400>;
			low_limit = <400>;
			high_limit = <600>;
			dirty_low_step = <200>;
			dirty_high_step = <400>;
			ps_dirty_limit = <1023>;
			ps_ir_limit = <500>;
			ps_adjust_min = <0>;
			ps_adjust_max = <600>;
			sampling_count = <5>;
			step_max = <400>;
			step_min = <100>;
			step_div = <3000>;
			anti_shake_delta = <55>;
			dynamic_cali_max = <6000>;
			raw2offset_radio = <1000>;
			offset_max = <60000>;
			offset_range_min = <0>;
			offset_range_max = <65534>;
			force_cali_limit = <6000>;
			cali_jitter_limit = <20>;
			cal_offset_margin = <3000>;
			bootup_cali_max = <20000>;
			sensor-reg = <0x03 0x20 0xA1 0x78>;
		};
		light_1: light_1 {
			status = "okay";
			sensor_name = <9>;//TCS3720
			i2c_num = <5>;
			i2c_addr = <0x39>;
			direction = <0>;
			eint_num = <19>;
			als_ratio = <110>;
			cct_type = <0x02>; //0x01 CCT_NORMAL 0x02 CCT_WISE
			device_name = "tcs3720";
			ir_ratio = <8>; // *1000
			gold_cct_factor = <1000 1000 1000 1000 1001 1001>;
			available_sensor = <12>;
			als_delay = <300>;
			high_pwm_cct = <1>;
			max_rpt_angle = <70>;
		};
		als_panel_P_3_1: als_panel_P_3_1 {
			status = "okay";
			device_name = "tcs3720";
			decision_table_index = <5>;
			support_panel = <7>;
			gold_cct_3k = <2409 1182 575 3848 1001 1001>;
			gold_cct_6k = <1560 1262 748 3321 1001 1001>;
			atime = <50>;
			scale_gain = <512>;
		};
		als_panel_samsung_1: als_panel_samsung_1 {
			status = "okay";
			device_name = "tcs3720";
			decision_table_index = <6>;
			support_panel = <1>; // 1-SAMSUNG, 2-BOE, 3-TIANMA
			gold_cct_3k = <6441 3223 1592 10121 1001 1001>;
			gold_cct_6k = <4350 3477 1951 8865 1001 1001>;
			atime = <25>;
			scale_gain = <512>;
		};
		/*
		enum {
			STK37600,
			MN29005,
			TCS3408,
			TCS3449,
		};
		*/
		rear_cct_1: rear_cct_1 {
			status = "okay";
			sensor_name = <3>;//TCS3449
			i2c_num = <0>;
			i2c_addr = <0x59>;
			available_sensor = <23>; // bit 0: rear als, bit 1: rear cct, bit 2: rear flicker, bit 3: rear proximity
			device_name = "tcs3449";
			cct_coef_Hir = <28073000 1 28853000 0 25804000 1 1703000 0
							26384000 1 27124000 0 19232000 1 1027000 0
							10716000 1 8010000 0 31176000 0 365000 1>;
			cct_coef_Lir = <31772000 0 9486000 1 31860000 0 118193000 1
							23055000 0 5374000 1 30599000 0 89059000 1
							13033000 0 7999000 1 65764000 0 77206000 1>;
			gold_rear_spectrum_3k = <1716 4011 4169 5065 8243 1210 1783 2648 4698 924 6254 3699 1011>; // Z Y X NIR VIS F2 F3 F4 F6 F1 F7 F8 F5 F
			gold_rear_spectrum_6k = <2935 4009 3761 4388 7165 2295 2387 3135 3977 1937 4513 1982 1020>;
		};
		/*
		enum {
			SX933x
		};
		*/
		sar_1: sar_1 {
			status = "okay";
			sensor_name = <0>;//SX933x
			i2c_num = <6>;
			i2c_addr = <0x28>;
			direction = <4>;
			eint_num = <11>;
			channel_num = <1>;
			channel_0_min = <3243>;
			channel_0_max = <4865>;
			channel_1_min = <0>;
			channel_1_max = <70000>;
			channel_2_min = <0>;
			channel_2_max = <70000>;
			channel_3_min = <0>;
			channel_3_max = <70000>;
			reg_table_num = <5>;
			use_far_near = <1>;
		};
		/*
		enum {
			MXM1120,
		};
		*/
		hall_1: hall_1 {
			status = "okay";
			sensor_name = <0>;//MXM1120
			i2c_num = <2>;
			i2c_addr = <0x0D>;
			direction = <0>;
			eint_num = <4>;
			device_name = "mxm1120";
			fold_hall_limit = <0 100 0 200 150 600 0 0 0 0>;
		};

		pick_up: pick_up {
			status = "okay";
			use-proximity = <1>;
		};
		fp_display: fp_display {
			status = "okay";
			unuse_axis_z = <1>;
		};
		lux_aod: lux_aod {
			status = "okay";
			use-proximity = <1>;
		};
		alsps_arch: alsps_arch {
			status = "okay";
			new-arch-supported = <1>;
		};
		cali_arch: cali_arch {
			status = "okay";
			new-arch-supported = <1>;
		};
		oplus_fusion: oplus_fusion {
			status = "okay";
			use_sub_accgyro = <1>;
		};
		smart_prox: smart_prox {
			status = "okay";
			support_smart_prox = <1>;
			use_sub_accgyro = <1>;
		};
		hinge_detect: hinge_detect {
			status = "okay";
			fold_threshold_gap = <70>;
			fold_threshold_default = <60>;
			fold_threshold_limit = <200>;
		};
		ultrasound_prox: ultrasound_prox {
			status = "okay";
			support_ultrasound_prox = <1>;
		};
		pocket: pocket {
			status = "okay";
			support_pocket = <1>;
			use_sub_accgyro = <1>;
		};
		gesture_prox: gesture_prox {
			status = "okay";
			support_gesture_prox = <1>;
			use_sub_accgyro = <1>;
		};
		dev_ori: dev_ori {
			status = "okay";
			support_dev_ori = <1>;
			use_sub_accgyro = <1>;
		};
		sub_dev_ori: sub_dev_ori {
			status = "okay";
			support_sub_dev_ori = <1>;
			use_sub_accgyro = <1>;
		};
		ui_dev_ori: ui_dev_ori {
			status = "okay";
			support_ui_dev_ori = <1>;
			use_sub_accgyro = <1>;
		};
	};
/* sensor end */
};

&mt6373_vibr {
	regulator-min-microvolt = <3000000>;
	regulator-max-microvolt = <3000000>;
	regulator-always-on;
};